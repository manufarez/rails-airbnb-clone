
    <div class="col-sm-6">
      <div class="booking">

      <%= simple_form_for ([ sozee, booking ]) do |f| %>
        <%= f.input :location %>
        <%= f.input :starts_at, as: :string, required: false, input_html: {class: "datepicker"} %>
        <%= f.input :ends_at, as: :string, required: false, input_html: {class: "datepicker"} %>
        <%= f.submit "Book this sozee !", class: "btn btn-primary" %>
      <% end %>
      </div>

    </div>
